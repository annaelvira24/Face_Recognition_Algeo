<LandingPage>
    FloatLayout:
        Button:
            text: "Getting Started"
            size_hint:(0.25,0.10)
            pos: (300,300)
            on_press: root.manager.current='start'
        Button: 
            text: "Credits"
            size_hint:(0.25,0.10)
            pos: (300,200)
            on_press: root.manager.current='credits'
        Button:
            text : "Quit"
            size_hint:(0.25,0.10)
            pos: (300,100)
            on_press: app.Exit()

